{
    "name": "emichalsen/RedirectFinder",
    "description": "Find redirects in site.",
    "authors": [
        {
            "name": "Eric Michalsen",
            "email": "eric.michalsen@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "slim/twig-view": "^2.3",
        "gabordemooij/redbean": "^4.3",
        "guzzlehttp/guzzle": "^6.3",
        "maknz/slack": "^1.7",
        "paquettg/php-html-parser": "^1.7",
        "yeriomin/redbean-single-file": "^4.3",
        "rogercreasy/slim-dot-env": "dev-master",
        "vlucas/phpdotenv": "^2.4",
        "doctrine/collections": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public public/index.php",
        "test": "phpunit"
    }

}
